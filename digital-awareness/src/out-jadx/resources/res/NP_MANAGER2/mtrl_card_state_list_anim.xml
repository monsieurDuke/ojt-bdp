<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:state_enabled="true"
        app:state_dragged="true">
        <set>
            <objectAnimator
                android:interpolator="@NP_MANAGER10/mtrl_fast_out_slow_in"
                android:duration="@NP_MANAGER9/mtrl_card_anim_duration_ms"
                android:valueTo="@NP_MANAGER6/mtrl_card_dragged_z"
                android:valueType="floatType"
                android:propertyName="translationZ"
                android:startDelay="@NP_MANAGER9/mtrl_card_anim_delay_ms"/>
        </set>
    </item>
    <item android:state_enabled="true">
        <set>
            <objectAnimator
                android:interpolator="@NP_MANAGER1/mtrl_card_lowers_interpolator"
                android:duration="@NP_MANAGER9/mtrl_card_anim_duration_ms"
                android:valueTo="0dp"
                android:valueType="floatType"
                android:propertyName="translationZ"/>
        </set>
    </item>
</selector>
